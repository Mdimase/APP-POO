package Practica4Unnoba.Servicios;

import java.util.List;

import Practica4Unnoba.repositorios.InviteRepository;
import Practica4Unnoba.repositorios.UserRepository;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.stereotype.Service;
import Practica4Unnoba.Entidades.Invite;


@Service
public class InviteServicios {
	private InviteRepository inviteRepository;
	
	public void addInvite(Invite invite) {
	    inviteRepository.save(invite);
	    
	public void deleteInvite(@PathVariable Long id) {
		inviteRepository.deleteById(id);
}
